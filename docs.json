{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "PrimePix API",
  "domain": "docs.primepix.me",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "PrimePix API",
            "pages": [
              "api-reference/introduction",
              "api-reference/authentication",
              "api-reference/webhooks"
            ]
          },
          {
            "group": "Objetos de resposta",
            "pages": [
              "api-reference/objects/transaction",
              "api-reference/objects/customer",
              "api-reference/objects/pix"
            ]
          },
          {
            "group": "Transações PIX",
            "pages": [
              "api-reference/pix/create",
              "api-reference/pix/status"
            ]
          },
          {
            "group": "Conta",
            "pages": [
              "api-reference/account/balance",
              "api-reference/account/transactions",
              "api-reference/account/withdraw"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Dashboard",
